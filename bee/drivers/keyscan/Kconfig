config REALTEK_BEE_KEYSCAN
	bool "REALTEK Bee MCU keyscan driver"
	default y
	depends on USE_HAL_REALTEK_BEE_KEYSCAN

	select USE_HAL_REALTEK_BEE_RCC
	select USE_HAL_REALTEK_BEE_PINMUX
	select USE_HAL_REALTEK_BEE_NVIC

	help
	  This option enables the keyscan driver for REALTEK Bee MCU.